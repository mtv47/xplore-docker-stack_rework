# Environment variables for Control Station Docker Compose

# Display and X11 settings
DISPLAY=:0
XAUTH=/tmp/.docker.xauth

# Project paths
PARENT_DIR=..

# ROS Configuration
ROS_DISTRO=humble
RMW_IMPLEMENTATION=rmw_cyclonedds_cpp

# Network settings (if needed for specific configurations)
CS_FRONTEND_PORT=3000
ROSBRIDGE_PORT=9090

# Container settings
CONTAINER_NAME=cs_humble_desktop
IMAGE_TAG=humble-desktop

# User settings (adjust if needed)
USER_ID=1000
GROUP_ID=1000